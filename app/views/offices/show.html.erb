<p id="notice"><%= notice %></p>

<p>
  <b>Description:</b>
  <%= @office.desc %>
</p>

<p>
  <b>Number:</b>
  <%= @office.number %>
</p>

<p>
  <b>Building:</b>
  <%= @office.building != nil ? @office.building.desc: "" %>
</p>

<div class="button-group">
	<%= link_to 'Edit', edit_building_office_path(@office.building,@office), :class => "button primary" %>
	<%= link_to 'Back', building_path(@office.building), :class => "button danger" %>
</div>
